{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api-gateway",
  "projects": {
    "activity-service": {
      "type": "application",
      "root": "apps/activity-service",
      "entryFile": "main",
      "sourceRoot": "apps/activity-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/activity-service/tsconfig.app.json"
      }
    },
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "comment-service": {
      "type": "application",
      "root": "apps/comment-service",
      "entryFile": "main",
      "sourceRoot": "apps/comment-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/comment-service/tsconfig.app.json"
      }
    },
    "logging-service": {
      "type": "application",
      "root": "apps/logging-service",
      "entryFile": "main",
      "sourceRoot": "apps/logging-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/logging-service/tsconfig.app.json"
      }
    },
    "notification-service": {
      "type": "application",
      "root": "apps/notification-service",
      "entryFile": "main",
      "sourceRoot": "apps/notification-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification-service/tsconfig.app.json"
      }
    },
    "organization-service": {
      "type": "application",
      "root": "apps/organization-service",
      "entryFile": "main",
      "sourceRoot": "apps/organization-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/organization-service/tsconfig.app.json"
      }
    },
    "project-service": {
      "type": "application",
      "root": "apps/project-service",
      "entryFile": "main",
      "sourceRoot": "apps/project-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/project-service/tsconfig.app.json"
      }
    },
    "task-service": {
      "type": "application",
      "root": "apps/task-service",
      "entryFile": "main",
      "sourceRoot": "apps/task-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/task-service/tsconfig.app.json"
      }
    },
    "user-service": {
      "type": "application",
      "root": "apps/user-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-service/tsconfig.app.json"
      }
    }
  }
}
